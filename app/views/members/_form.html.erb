
  <%= form_for(@member) do |f| %>
    <div class="field">
      <%= f.label :user_name %><br>
      <%= f.text_field :user_name %>
    </div>
    <div class="field">
      <%= f.label :rank %><br>
      <%= f.select(:rank, ["Leader", "Co-Leader", "Elder", "Member"]) %>
    </div>
    <div class="actions">
      <%= f.submit 'Add Member', :class => 'btn btn-success' %>
    </div>
  <% end %>



